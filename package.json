{
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm caches/blog/*",
    "deplock": "rm package-lock.json && rm -rf node_modules && npm install",
    "dev": "next dev",
    "hook": "npm run unhook && find git-hooks -maxdepth 1 -type f -exec chmod +x {} \\; -exec ln -sf $(pwd)/{} .git/hooks \\;",
    "lint": "eslint '**/*.ts*'",
    "lint-and-fix": "eslint --fix '**/*.ts*'",
    "prettier-format": "prettier --config .prettierrc --write '**/*.ts*'",
    "sandbox": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'sandbox.ts'",
    "tests": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "unhook": "find .git/hooks -type l -exec unlink {} \\;",
    "update": "ncu"
  },
  "dependencies": {
    "@octokit/graphql": "^7.0.2",
    "@octokit/types": "^12.3.0",
    "@vercel/analytics": "^1.1.1",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "next": "^14.0.3",
    "next-share": "^0.27.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype": "^12.0.1",
    "rehype-document": "^7.0.2",
    "rehype-format": "^5.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^9.0.3",
    "remark": "^15.0.1",
    "remark-gfm": "^3.0.1",
    "remark-html": "^16.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^11.0.0",
    "simple-git": "^3.20.0",
    "to-vfile": "^8.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.10",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.12.0",
    "chai": "^4.3.10",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.14.6",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
