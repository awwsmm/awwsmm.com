{
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm caches/blog/*",
    "deplock": "rm package-lock.json && rm -rf node_modules && npm install",
    "dev": "next dev",
    "hook": "npm run unhook && find git-hooks -maxdepth 1 -type f -exec chmod +x {} \\; -exec ln -sf $(pwd)/{} .git/hooks \\;",
    "lint": "eslint $(npm run --silent list-new-and-changed-tsx-files)",
    "lint-and-fix": "eslint --fix $(npm run --silent list-new-and-changed-tsx-files)",
    "list-new-and-changed-tsx-files": "git diff --name-only --cached --diff-filter=d '*.tsx'",
    "prettier-format": "prettier --config .prettierrc --write $(npm run --silent list-new-and-changed-tsx-files)",
    "sandbox": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'sandbox.ts'",
    "tests": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "unhook": "find .git/hooks -type l -exec unlink {} \\;"
  },
  "dependencies": {
    "@octokit/graphql": "^6.0.0",
    "@octokit/types": "^9.2.3",
    "@vercel/analytics": "^1.0.1",
    "date-fns": "^2.30.0",
    "gray-matter": "^4.0.3",
    "next": "^13.4.6",
    "next-share": "^0.20.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype": "^12.0.1",
    "rehype-document": "^6.1.0",
    "rehype-format": "^4.0.1",
    "rehype-highlight": "^6.0.0",
    "rehype-stringify": "^9.0.3",
    "remark": "^14.0.3",
    "remark-gfm": "^3.0.1",
    "remark-html": "^15.0.2",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0",
    "simple-git": "^3.19.0",
    "to-vfile": "^7.2.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.59.11",
    "chai": "^4.3.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.2.0",
    "npm-check-updates": "^16.10.12",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "engines": {
    "node": ">=16.8.0"
  }
}
